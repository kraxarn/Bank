<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="Bank.Views.ErrorPage">

	<ContentPage.Content>

		<StackLayout
			Margin="16">

			<Label
				x:Name="LabelTitle"
				FontAttributes="Bold"
				FontSize="16"
                VerticalOptions="CenterAndExpand"/>
			
			<Label
				Margin="0 8 0 0"
				x:Name="LabelDetails"/>

		</StackLayout>

	</ContentPage.Content>

</ContentPage>